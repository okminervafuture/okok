<Page x:Class="WiseScopeDemo.New.Pages.PageAll"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WiseScopeDemo.New.Pages"
      xmlns:control="clr-namespace:WiseScopeDemo.New"
      mc:Ignorable="d" 
      d:DesignHeight="5000"
       d:DesignWidth="500"
      Title="PageAll"  Loaded="Page_Loaded" Foreground="Black" Background="White"  >

    <ScrollViewer x:Name="scroll" VerticalScrollBarVisibility="Auto">

        <Grid Margin="10">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <GroupBox x:Name="groupBox" Header=" Changing Step" >
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Label x:Name="label" Content="MALS의 Step을 변경합니다." Foreground="#FF7D7D7D" Grid.ColumnSpan="4" VerticalAlignment="Center"/>
                    <Label x:Name="label_Copy3" Content="Set Value(Step)" VerticalAlignment="Center" Foreground="#FF151414" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Center"/>
                    <Button x:Name="StepChangebtn" Content="send" Click="StepChangebtn_Click" Grid.Column="3" Grid.Row="2" Margin="20,5,20,5" />
                    <Slider x:Name="SliderStepChangeValue" Maximum="320"  TickFrequency="10" SmallChange="10" IsSnapToTickEnabled="True" Grid.Column="1" Grid.Row="2" VerticalAlignment="Center" Margin="20,0,20,0"/>
                    <TextBox x:Name="TextBoxStepChangeValue" TextWrapping="Wrap" Text="" Grid.Column="2" Grid.Row="2" Margin="20,5,20,5" />
                    <Label x:Name="label_Copy1" Content="Command" Foreground="#FF151414" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Label x:Name="label_Copy36" Content="Present Value" Foreground="#FF151414" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <TextBlock x:Name="textBlock_ChangeStep_PresentValue" TextWrapping="Wrap" Text="TextBlock" Grid.Row="2" HorizontalAlignment="Center" VerticalAlignment="Center" />
                    <Label x:Name="label14" Content="Min:0, Max:320" Grid.Column="2" Grid.Row="1"  VerticalAlignment="Center" HorizontalAlignment="Center"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox1" Header="Setting Coaxial On Time" Grid.Row="1">
                <Grid>
                    <Label x:Name="label_Copy34" Content="Set Value(time)" HorizontalAlignment="Left" Height="24" Margin="111,35,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy35" Content="Command" HorizontalAlignment="Left" Height="24" Margin="393,27,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy" Content="동축 조명이 켜져 있는 시간을 설정합니다." HorizontalAlignment="Left" Height="24" Margin="4,5,0,0" VerticalAlignment="Top" Width="330" Foreground="#FF7D7D7D" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="TextBoxSetCoaxialOnTimeValue" HorizontalAlignment="Left" Height="22" Margin="126,65,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="73" TextChanged="coaxialontime_TextChanged" />
                    <Label x:Name="label_Copy7" Content="32-5000" HorizontalAlignment="Left" Height="24" Margin="207,65,0,0" VerticalAlignment="Top" Width="58" Foreground="#FF151414"/>
                    <Button x:Name="coaxialtimesend" Content="send" HorizontalAlignment="Left" Margin="405,62,0,0" VerticalAlignment="Top" Width="43" Click="coaxialtimesend_Click" Height="19" />
                    <Label x:Name="label_Copy38" Content="Present Value" HorizontalAlignment="Left" Height="24" Margin="9,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <TextBlock x:Name="textBlock_CoxialOntime_PresentValue" HorizontalAlignment="Left" Margin="16,60,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="30" Width="80"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox2" Header="Setting Ring On Time" Grid.Row="2">
                <Grid>
                    <Label x:Name="label_Copy4" Content="Set Value(time)" HorizontalAlignment="Left" Height="24" Margin="110,34,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy6" Content="Command" HorizontalAlignment="Left" Height="24" Margin="401,26,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy37" Content="32-5000" HorizontalAlignment="Left" Height="24" Margin="207,61,0,0" VerticalAlignment="Top" Width="55" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy5" Content="링 조명이 켜져 있는 시간을 설정합니다." HorizontalAlignment="Left" Height="24" Margin="5,7,0,0" VerticalAlignment="Top" Width="227" Foreground="#FF7D7D7D" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="TextBoxSetRingOnTimeValue" HorizontalAlignment="Left" Height="24" Margin="126,61,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="71" TextChanged="ringontime_TextChanged"/>
                    <Button x:Name="ringtimesend" Content="send" HorizontalAlignment="Left" Margin="402,57,0,0" VerticalAlignment="Top" Width="44" Click="ringtimesend_Click" Height="24"/>
                    <Label x:Name="label_Copy39" Content="Present Value" HorizontalAlignment="Left" Height="24" Margin="9,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <TextBlock x:Name="textBlock_RingOntime_PresentValue" HorizontalAlignment="Left" Margin="16,60,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="30" Width="80"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox3" Header="Selecting Coaxial Light" Grid.Row="3">
                <Grid>
                    <Label x:Name="label_Copy10" Content="Command" HorizontalAlignment="Left" Height="24" Margin="397,23,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label1" Content="동축 조명을 선택합니다." HorizontalAlignment="Left" Margin="1.8,8,0,0" VerticalAlignment="Top" Width="147" Height="26" Foreground="#FF7D7D7D" Grid.Column="1"/>
                    <Button x:Name="SelectCoaxial" Content="동축 조명" HorizontalAlignment="Left" Height="26" Margin="400,57,0,0" VerticalAlignment="Top" Width="54" Click="SelectCoaxial_Click"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox4" Header="Selecting Ring Light" Grid.Row="4">
                <Grid>
                    <Label x:Name="label_Copy9" Content="Command" HorizontalAlignment="Left" Height="24" Margin="393,22,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label2" Content="링 조명을 선택합니다." HorizontalAlignment="Left" Margin="7,6,0,0" VerticalAlignment="Top" Width="134" Height="26" Foreground="#FF7D7D7D" Grid.ColumnSpan="2"/>
                    <Button x:Name="SelectRing" Content="링 조명" HorizontalAlignment="Left" Height="26" Margin="403,50,0,0" VerticalAlignment="Top" Width="46" Click="SelectRing_Click"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox5" Header="Setting Step In Table" Grid.Row="5" >
                <Grid>
                    <Label x:Name="label_Copy13" Content="Set Value(Step)" HorizontalAlignment="Left" Height="26" Margin="186,4,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Slider x:Name="sliderSetStepInTableValue" HorizontalAlignment="Left" Margin="191,28,0,0" VerticalAlignment="Top" Width="78" Maximum="320" Value="{Binding FocusStep, Mode=TwoWay}" TickFrequency="10" SmallChange="10" IsSnapToTickEnabled="True" Height="18"/>
                    <TextBox x:Name="TextBoxSetStepInTableStepValue" HorizontalAlignment="Left" Height="25" Margin="279,24,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="40"/>
                    <Label x:Name="label_Copy14" Content="Command" HorizontalAlignment="Left" Height="24" Margin="411,29,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Button x:Name="coaxialtimesend_Copy1" Content="send" HorizontalAlignment="Left" Margin="428,65,0,0" VerticalAlignment="Top" Width="32"  Height="19" />
                    <Label x:Name="label3" Content="테이블에 Step값을 저장합니다." HorizontalAlignment="Left" Margin="12,8,0,0" VerticalAlignment="Top" Height="26" Width="178" Grid.ColumnSpan="2" Foreground="#FF7D7D7D"/>
                    <TextBox x:Name="TextBoxSetStepInTableIndexValue" HorizontalAlignment="Left" Height="24" Margin="280,66,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="39"/>
                    <Label x:Name="label_Copy15" Content="SelectIndex" HorizontalAlignment="Left" Height="24" Margin="187,42,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Slider x:Name="sliderSetStepInTableIndexValue" HorizontalAlignment="Left" Margin="191,70,0,0" VerticalAlignment="Top" Width="72" Maximum="50" Value="{Binding FocusStep, Mode=TwoWay}" SmallChange="1" IsSnapToTickEnabled="True" Height="16" LargeChange="10"/>
                    <Label x:Name="label14_Copy" Content="0-50" HorizontalAlignment="Left" Margin="333,65,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label14_Copy1" Content="0-320(10단위)" HorizontalAlignment="Left" Margin="323,21,0,0" VerticalAlignment="Top" Height="24"/>
                    <Label x:Name="label_Copy40" Content="Present Value" HorizontalAlignment="Left" Height="24" Margin="9,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <TextBlock x:Name="textBlock_Copy2" HorizontalAlignment="Left" Margin="16,60,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="30" Width="80"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox6" Header="Setting Trigger Signal1" Grid.Row="6">
                <Grid>
                    <Label x:Name="label_Copy17" Content="Command" HorizontalAlignment="Left" Height="24" Margin="392,35,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label4" Content="G Command에서 설정한 index0에 저장된 값으로 MALS의 focus를 변경하고 H/W Trigger Signal을 보내서 MALS와 카메라를 연동시킨다."  HorizontalAlignment="Left"  VerticalAlignment="Top"  Grid.ColumnSpan="3" Foreground="#FF7D7D7D"/>
                    <Button x:Name="TriggerSignal1" Content="send" HorizontalAlignment="Left" Height="24" Margin="416,63,0,0" VerticalAlignment="Top" Width="32" Click="TriggerSignal1_Click"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox7" Header="Setting Trigger Signal2" Grid.Row="7">
                <Grid Margin="6,0,-6,0.2">
                    <Label x:Name="label_Copy16" Content="Command" HorizontalAlignment="Left" Height="24" Margin="405,37,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label5" Content="사용자가 설정한 값으로 MALS의 step을 변경한 뒤 동축조명과 링조명을 순서대로 켜서 H/W Trigger Signal을 보내  MALS와 카메라를 연동시킨다." HorizontalAlignment="Left"  VerticalAlignment="Top"  Grid.ColumnSpan="3" Foreground="#FF7D7D7D"/>
                    <Button x:Name="TriggerSignal22btn" Content="send" HorizontalAlignment="Left" Margin="415,69,0,0" VerticalAlignment="Top" Width="32"  Height="19" Click="TriggerSignal22btn_Click" />
                    <ComboBox x:Name="TriggerSignal22" HorizontalAlignment="Left" Margin="225,36,0,0" VerticalAlignment="Top" Width="66" SelectionChanged="TriggerSignal22_SelectionChanged" Height="22"/>
                    <Label x:Name="label_Copy18" Content="Set Value(Step)" HorizontalAlignment="Left" Height="24" Margin="122,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <Slider x:Name="sliderTriggerSignal2_StepValue" HorizontalAlignment="Left" Margin="125,68,0,0" VerticalAlignment="Top" Width="114" Maximum="320" Value="{Binding FocusStep, Mode=TwoWay}" TickFrequency="10" SmallChange="10" IsSnapToTickEnabled="True" Height="20"/>
                    <TextBox x:Name="TextBoxTriggerSignal2_StepValue" HorizontalAlignment="Left" Height="26" Margin="253,65,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="58"/>
                    <Label x:Name="label14_Copy2" Content="0-320(10단위)" HorizontalAlignment="Left" Margin="309,66,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label_Copy41" Content="Present Value" HorizontalAlignment="Left" Height="24" Margin="9,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <TextBlock x:Name="textBlock_triggersignal2_presentvalue" HorizontalAlignment="Left" Margin="16,60,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="30" Width="80"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox8" Header="Setting Trigger Signal3" Grid.Row="8">
                <Grid>
                    <Label x:Name="label_Copy19" Content="Command" HorizontalAlignment="Left" Height="24" Margin="377,32,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label6" Content="외부의 신호를 받고 동축조명과 링조명을 순서대로 켜서 H/W Trigger Signal을 보내 MALS와 카메라를 연동시킨다." HorizontalAlignment="Left"  VerticalAlignment="Top"  Grid.ColumnSpan="3" Foreground="#FF7D7D7D"/>
                    <Button x:Name="TriggerSignal3" Content="send" HorizontalAlignment="Left" Height="23" Margin="401,65,0,0" VerticalAlignment="Top" Width="46" Click="TriggerSignal3_Click"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox9" Header="Setting Trigger Signal4" Grid.Row="9">
                <Grid>
                    <Label x:Name="label_Copy20" Content="Command" HorizontalAlignment="Left" Height="24" Margin="393,34,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy21" Content="Set Value(Step)" HorizontalAlignment="Left" Height="24" Margin="14,40,0,0" VerticalAlignment="Top" Width="101" Foreground="#FF151414"/>
                    <Label x:Name="label7" Content="사용자가 설정한 값으로 MALS의 step을 변경한 뒤 동축조명을 켜서 H/W Trigger Signal을 보내 MALS와 카메라를 연동시킨다." HorizontalAlignment="Left" Margin="7,7,0,0" VerticalAlignment="Top" Height="26" Width="405" Grid.ColumnSpan="3" Foreground="#FF7D7D7D"/>
                    <ComboBox x:Name="TriggerSignal44" HorizontalAlignment="Left" Margin="157,41,0,0" VerticalAlignment="Top" Width="58"  Height="22" />
                    <Button x:Name="TriggerSignal44btn" Content="send" HorizontalAlignment="Left" Margin="422,65,0,0" VerticalAlignment="Top" Width="32"  Height="19" Click="TriggerSignal44btn_Click" />
                    <Slider x:Name="sliderTriggerSignal4_StepValue" HorizontalAlignment="Left" Margin="32,78,0,0" VerticalAlignment="Top" Width="125" Maximum="320" Value="{Binding FocusStep, Mode=TwoWay}" TickFrequency="10" SmallChange="10" IsSnapToTickEnabled="True" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="TextBoxTriggerSignal4_StepValue" HorizontalAlignment="Left" Height="26" Margin="163,73,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="58"/>
                    <Label x:Name="label14_Copy3" Content="0-320(10단위)" HorizontalAlignment="Left" Margin="233,77,0,-2.8" VerticalAlignment="Top"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox10" Header="Setting Trigger Signal5" Grid.Row="10">
                <Grid>
                    <Label x:Name="label_Copy22" Content="Command" HorizontalAlignment="Left" Height="24" Margin="383,36,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy23" Content="Set Value(Step)" HorizontalAlignment="Left" Height="24" Margin="26,40,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414" Grid.ColumnSpan="2"/>
                    <Label x:Name="label7_Copy" Content="사용자가 설정한 값으로 MALS의 step을 변경한 뒤 링조명을 켜서 H/W Trigger Signal을 보내 MALS와 카메라를 연동시킨다." HorizontalAlignment="Left" Margin="9,11,0,0" VerticalAlignment="Top" Height="26" Width="394" Grid.ColumnSpan="3" Foreground="#FF7D7D7D"/>
                    <ComboBox x:Name="TriggerSignal55" HorizontalAlignment="Left" Margin="130,39,0,0" VerticalAlignment="Top" Width="57"  Height="22" />
                    <Button x:Name="TriggerSignal55btn" Content="send" HorizontalAlignment="Left" Margin="409,70,0,0" VerticalAlignment="Top" Width="32"  Height="19" Click="TriggerSignal55btn_Click" />
                    <Slider x:Name="sliderTriggerSignal5_StepValue" HorizontalAlignment="Left" Margin="33,72,0,0" VerticalAlignment="Top" Width="125" Maximum="320" Value="{Binding FocusStep, Mode=TwoWay}" TickFrequency="10" SmallChange="10" IsSnapToTickEnabled="True" Grid.ColumnSpan="2"/>
                    <TextBox x:Name="TextBoxTriggerSignal5_StepValue" HorizontalAlignment="Left" Height="26" Margin="164,70,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="58"/>
                    <Label x:Name="label14_Copy4" Content="0-320(10단위)" HorizontalAlignment="Left" Margin="232,70,0,0" VerticalAlignment="Top"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox11" Header="Setting Trigger Delay Time" Grid.Row="11">
                <Grid>
                    <Label x:Name="label_Copy24" Content="Command" HorizontalAlignment="Left" Height="24" Margin="391,39,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy25" Content="Set Value(time)" HorizontalAlignment="Left" Height="24" Margin="144,35,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Label x:Name="label8" Content="사용자가 설정한 값으로 MALS의 step을 변경한 뒤 H/W Trigger를 얼마나 지연시킬 지 설정한다." HorizontalAlignment="Left"  VerticalAlignment="Top" Foreground="#FF7D7D7D"/>
                    <ComboBox x:Name="SignalDelay" HorizontalAlignment="Left" Height="24" Margin="248,34,0,0" VerticalAlignment="Top" Width="45"  />
                    <Button x:Name="SignalDelaybtn" Content="send" HorizontalAlignment="Left" Margin="422,71,0,0" Width="32" Click="SignalDelaybtn_Click" Height="20" VerticalAlignment="Top" />
                    <Slider x:Name="SliderTriggerDelayTime_TimeValue" HorizontalAlignment="Left" Margin="142,67,0,0" VerticalAlignment="Top" Width="125" Maximum="100" Value="{Binding FocusStep, Mode=TwoWay}" SmallChange="1" IsSnapToTickEnabled="True" LargeChange="10"/>
                    <TextBox x:Name="TextBoxTriggerDelayTime_TimeValue" HorizontalAlignment="Left" Height="26" Margin="278,66,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="58"/>
                    <Label x:Name="label14_Copy5" Content="0-100" HorizontalAlignment="Left" Margin="339,64,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label_Copy42" Content="Present Value" HorizontalAlignment="Left" Height="24" Margin="9,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <TextBlock x:Name="textBlock_triggerdelaytime_PresentValue" HorizontalAlignment="Left" Margin="16,60,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="30" Width="80"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox12" Header="Setting Trigger Hold Time" Grid.Row="12">
                <Grid>
                    <Label x:Name="label_Copy26" Content="Command" HorizontalAlignment="Left" Height="24" Margin="381,35,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy27" Content="Set Value(time)" HorizontalAlignment="Left" Height="24" Margin="131,35,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Label x:Name="label9" Content="Trigger를 보낼 수 있는 상태가 되었을 때 얼마 후에 신호를 보낼지 설정한다." HorizontalAlignment="Left" Margin="9,8,0,0" VerticalAlignment="Top" Height="26" Width="422" Grid.ColumnSpan="3" Foreground="#FF7D7D7D"/>
                    <ComboBox x:Name="SignalHold" HorizontalAlignment="Left" Height="24" Margin="238,38,0,0" VerticalAlignment="Top" Width="45"  />
                    <Button x:Name="SignalHoldbtn" Content="send" HorizontalAlignment="Left" Margin="420,67,0,0" VerticalAlignment="Top" Width="30"  Height="20" Click="SignalHoldbtn_Click" />
                    <Slider x:Name="SliderTriggerHoldTime_TimeValue" HorizontalAlignment="Left" Margin="137,71,0,0" VerticalAlignment="Top" Width="125" Maximum="50" Value="{Binding FocusStep, Mode=TwoWay}" SmallChange="1" IsSnapToTickEnabled="True" LargeChange="10"/>
                    <TextBox x:Name="TextBoxTriggerHoldTime_TimeValue" HorizontalAlignment="Left" Height="26" Margin="265,68,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="58"/>
                    <Label x:Name="label14_Copy6" Content="0-50" HorizontalAlignment="Left" Margin="332,67,0,0" VerticalAlignment="Top"/>
                    <Label x:Name="label_Copy43" Content="Present Value" HorizontalAlignment="Left" Height="24" Margin="9,33,0,0" VerticalAlignment="Top" Width="96" Foreground="#FF151414"/>
                    <TextBlock x:Name="textBlock_triggerholdtime_PresentValue" HorizontalAlignment="Left" Margin="16,60,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="30" Width="80"/>
                </Grid>
            </GroupBox>
            <GroupBox x:Name="groupBox13" Header="Changing Step For Image Capture" Grid.Row="13">
                <Grid>
                    <Label x:Name="label_Copy28" Content="Command" HorizontalAlignment="Left" Height="24" Margin="390,37,0,0" VerticalAlignment="Top" Width="76" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy29" Content="Set Value(Step)" HorizontalAlignment="Left" Height="24" Margin="114,37,0,0" VerticalAlignment="Top" Width="146" Foreground="#FF151414"/>
                    <Label x:Name="label_Copy2" Content="이미지 캡처를 위한 MALS step을 설정한다." HorizontalAlignment="Left" Height="27" Margin="15,8,0,0" VerticalAlignment="Top" Width="247" Grid.ColumnSpan="2" Foreground="#FF7D7D7D"/>
                    <ComboBox x:Name="TriggerCount" HorizontalAlignment="Left" Height="24" Margin="221,39,0,0" VerticalAlignment="Top" Width="45"/>
                    <Button x:Name="TriggerCountbtn" Content="send" HorizontalAlignment="Left" Margin="423,67,0,0" VerticalAlignment="Top" Width="32"  Height="19" Click="TriggerCountbtn_Click" />
                    <Slider x:Name="SliderImageCapture_StepValue" HorizontalAlignment="Left" Margin="116,66,0,0" VerticalAlignment="Top" Width="125" Maximum="320" Value="{Binding FocusStep, Mode=TwoWay}" TickFrequency="10" SmallChange="10" IsSnapToTickEnabled="True"/>
                    <TextBox x:Name="TextBoxImageCapture_StepValue" HorizontalAlignment="Left" Height="26" Margin="243,65,0,0" TextWrapping="Wrap" Text="{Binding FocusStep, Mode=TwoWay}" VerticalAlignment="Top" Width="58"/>
